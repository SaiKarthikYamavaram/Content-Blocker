{
	"manifest_version": 2,
	"name": "Content Blocker",
	"version": "0.0.1",
	"description": "A keyword based content blocker built with love ‚ù§ ",
	"icons": {
		"120": "icons/icon128.png",
		"48": "icons/icon48.png",
		"16": "icons/icon16.png"
	},
	"browser_action": {
		"default": "icon16.png",
		"default_popup": "popup.html"
	},
	"permissions": ["storage", "activeTab", "contextMenus"],
	"background": {
		"scripts": ["jquery-3.5.0.min.js"],
		"persistent": false
	},
	"commands": {
		"_execute_browser_action": {
			"suggested_key": {
				"default": "Ctrl+Shift+F",
				"mac": "MacCtrl+Shift+F"
			},
			"description": "Opens Content Blocker"
		}
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["access.js"]
		}
	]
}
